{
    "version": "1.5.0-beta",
    "compilerOptions": {
        "target": "es6",
        "module": "system",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/**/*"
    ],
    "files": [
        "./RuntimeFunctionGenerator.ts",
        "./arraymodel.ts",
        "./booleanmodel.ts",
        "./dispatcher.ts",
        "./genModelHelpers.ts",
        "./model.ts",
        "./objectmodel.ts",
        "./optionalmodel.ts",
        "./rootmodel.ts",
        "./stringmodel.ts",
        "./test.ts"
    ]
}
